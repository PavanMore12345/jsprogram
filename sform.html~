<html>

<head>

    <style>
        body {
	background-color: gray;
            text-align: center;
        }
    </style>
</head>

<body onload="getString()">
    <h1>StudentData</h1>
    <p id="sname"></p>
    <p id="sroll"></p>
    <p id="sage"></p>
    <p id="scity"></p>
    <script>
function getString() {
    var vars = [],
        hash;
    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
document.write(hashes);
    for (var i = 0; i < hashes.length; i++) {
        hash = hashes[i].split('=');
        vars.push(hash[0]);
        vars[hash[0]] = hash[1];

    }
document.write(vars);
    return vars;
}
var strings = getString();
document.getElementById('sname').innerHTML = "StudentName:  " + strings['sname'];
document.getElementById('sroll').innerHTML = "StudentRoll:  " + strings['sroll'];
document.getElementById('sage').innerHTML = "StudentAge:  " + strings['sage'];
document.getElementById('scity').innerHTML = "StudentCity:  " + strings['scity'];</script>
</body>

</html>
